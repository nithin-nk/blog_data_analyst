# Google Gemini API Keys (up to 5 keys for quota management)
# Get keys from: https://aistudio.google.com/apikey

GOOGLE_API_KEY_1=your_first_api_key_here
GOOGLE_API_KEY_2=your_second_api_key_here
GOOGLE_API_KEY_3=your_third_api_key_here
GOOGLE_API_KEY_4=your_fourth_api_key_here
GOOGLE_API_KEY_5=your_fifth_api_key_here

# ============================================================================
# LLM Model Configuration (Optional - defaults provided if not set)
# ============================================================================
# Configure which model and temperature to use for each node's LLM calls.
# Defaults: gemini-2.5-flash-lite (lite), gemini-2.5-flash (full)
# Temperature ranges: 0.3 (low, deterministic), 0.7 (medium, creative)

# Topic Discovery (Phase 0.5) - Generate search queries
TOPIC_DISCOVERY_MODEL=gemini-2.5-flash-lite
TOPIC_DISCOVERY_TEMPERATURE=0.7

# Content Landscape Analysis (Phase 0.7) - Analyze existing articles
CONTENT_LANDSCAPE_ANALYZE_MODEL=gemini-2.5-flash-lite
CONTENT_LANDSCAPE_ANALYZE_TEMPERATURE=0.3

# Content Landscape Strategy (Phase 0.7) - Determine content strategy
CONTENT_LANDSCAPE_STRATEGY_MODEL=gemini-2.5-flash-lite
CONTENT_LANDSCAPE_STRATEGY_TEMPERATURE=0.7

# Planning Outline (Phase 1) - Create blog outline
PLANNING_OUTLINE_MODEL=gemini-2.5-flash-lite
PLANNING_OUTLINE_TEMPERATURE=0.7

# Planning Uniqueness (Phase 1) - Check uniqueness vs analyzed articles
PLANNING_UNIQUENESS_MODEL=gemini-2.5-flash
PLANNING_UNIQUENESS_TEMPERATURE=0.3

# Planning Replanning (Phase 1) - Generate feedback for replanning
PLANNING_REPLANNING_MODEL=gemini-2.5-flash
PLANNING_REPLANNING_TEMPERATURE=0.3

# Research Alternative Queries (Phase 2) - Generate alternative search queries
RESEARCH_ALT_QUERIES_MODEL=gemini-2.5-flash-lite
RESEARCH_ALT_QUERIES_TEMPERATURE=0.7

# Validate Sources (Phase 2.5) - Filter and validate sources
VALIDATE_SOURCES_MODEL=gemini-2.5-flash-lite
VALIDATE_SOURCES_TEMPERATURE=0.3

# Write Section (Phase 3) - Write section content
WRITE_SECTION_MODEL=gemini-2.5-flash
WRITE_SECTION_TEMPERATURE=0.7

# Write Section Critic (Phase 3) - Critic evaluation
WRITE_SECTION_CRITIC_MODEL=gemini-2.5-flash-lite
WRITE_SECTION_CRITIC_TEMPERATURE=0.3

# Write Section Refine (Phase 3) - Refinement after critic
WRITE_SECTION_REFINE_MODEL=gemini-2.5-flash
WRITE_SECTION_REFINE_TEMPERATURE=0.3

# Final Assembly Critic (Phase 4) - Final critic evaluation
FINAL_ASSEMBLY_CRITIC_MODEL=gemini-2.5-flash
FINAL_ASSEMBLY_CRITIC_TEMPERATURE=0.3

# Final Assembly Transitions (Phase 4) - Fix transitions and rewrite
FINAL_ASSEMBLY_TRANSITIONS_MODEL=gemini-2.5-flash
FINAL_ASSEMBLY_TRANSITIONS_TEMPERATURE=0.7
